{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "rules": {
      "jsx-no-lambda": false,
      "member-access": false,
      "interface-name": true,
      "prefer-for-of": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "no-console": false
    },
    "linterOptions": {
      "exclude": [
        "node_modules/**/*.ts"
      ]
    }
}